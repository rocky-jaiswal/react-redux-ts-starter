{
  "name": "react-redux-ts-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts-ts": "2.8.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-sessionstorage": "^0.4.0",
    "seamless-immutable": "^7.1.2"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.5",
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.47",
    "@types/react": "^16.0.20",
    "@types/react-dom": "^16.0.2",
    "@types/react-intl": "^2.3.2",
    "@types/react-redux": "^5.0.12",
    "@types/react-router": "^4.0.15",
    "@types/react-router-dom": "^4.2.0",
    "@types/react-router-redux": "^5.0.7",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.2.2",
    "@types/seamless-immutable": "^7.1.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/containers/**/*.{ts,tsx}",
      "**/src/components/**/*.{ts,tsx}",
      "**/src/redux/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
